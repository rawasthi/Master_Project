<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/major_project</property>
    <property name="hibernate.connection.username">root</property>
    <property name="hibernate.connection.password">vish</property>
    <mapping class="net.ankur.spring3.entity.professorLogin"/>
    <mapping class="net.ankur.spring3.entity.AddCodeInfo"/>
    <mapping class="net.ankur.spring3.entity.SectionInfo"/>
    <mapping class="net.ankur.spring3.entity.StudentLogin"/>
    <mapping class="net.ankur.spring3.entity.Course"/>
    
  </session-factory>
</hibernate-configuration>
